    }
    wire_load("30x30") {
        resistance : 0 ;
        capacitance : 1 ;
        area : 0 ;
        slope : 0.782 ;
        fanout_length(1,1.40) ;
    }
    wire_load("40x40") {
        resistance : 0 ;
        capacitance : 1 ;
        area : 0 ;
        slope : 1.007 ;
        fanout_length(1,1.90) ;
    }
    wire_load("50x50") {
        resistance : 0 ;
        capacitance : 1 ;
        area : 0 ;
        slope : 1.218 ;
        fanout_length(1,1.80) ;
    }
    wire_load("60x60") {
        resistance : 0 ;
