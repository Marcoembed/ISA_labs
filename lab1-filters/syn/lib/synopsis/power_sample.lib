        area : 0 ;
        slope : 1.517 ;
        fanout_length(1,1.80) ;
    }
    wire_load("80x80") {
        resistance : 0 ;
        capacitance : 1 ;
        area : 0 ;
        slope : 1.590 ;
        fanout_length(1,1.80) ;
    }
    wire_load("90x90") {
        resistance : 0 ;
        capacitance : 1 ;
        area : 0 ;
        slope : 1.64 ;
        fanout_length(1,1.9) ;
    }

cell(AN2) {
  area : 2;
  pin(A) {
    direction : input;
    capacitance : 1;
  }
  pin(B) {
    direction : input;
    capacitance : 1;
  }
  pin(Z) {
    direction : output;
    function : "A B";
    timing() {
      intrinsic_rise : 0.48;
      intrinsic_fall : 0.77;
      rise_resistance : 0.1443;
      fall_resistance : 0.0523;
      slope_rise : 0.0;
      slope_fall : 0.0;
      related_pin : "A";
    }
    timing() {
      intrinsic_rise : 0.48;
      intrinsic_fall : 0.77;
      rise_resistance : 0.1443;
      fall_resistance : 0.0523;
      slope_rise : 0.0;
      slope_fall : 0.0;
      related_pin : "B";
    }
  }
                  /**********************************/
                  /* Output Power for Z Output      */
                  /**********************************/
		  cell_leakage_power : 1; 
		  internal_power(output_by_cap) { 
		     values("36.000000 , 5.000000 , 2.000000 ");
		     related_outputs : "Z"; 
		  }
}
