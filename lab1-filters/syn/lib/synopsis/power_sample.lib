cell(flop1) {
  area : 7;
  pin(D) {
    direction : input;
    capacitance : 1;
    timing() {
      timing_type : setup_rising;
      intrinsic_rise : 0.8;
      intrinsic_fall : 0.8;
      related_pin : "CP";
    }
    timing() {
      timing_type : hold_rising;
      intrinsic_rise : 0.4;
      intrinsic_fall : 0.4;
      related_pin : "CP";
    }
  }
  pin(CP) {
    direction : input;
    capacitance : 1;
    min_pulse_width_high : 1.5;
    min_pulse_width_low : 1.5;
  }
  ff(IQ, IQN) {
    next_state : "D";
    clocked_on : "CP";
  }

                  /**********************************/
                  /* Internal Power for Clock Input */
                  /**********************************/
		  internal_power(input_by_trans) { 
		     values("0.550000 , 0.600000 , .700000 ");
		     related_input : "CP"; 
		  }
  pin(Q) {
    direction : output;
    function : "IQ"
    timing() {
      timing_type : rising_edge;
      intrinsic_rise : 1.09;
      intrinsic_fall : 1.37;
      rise_resistance : 0.1458;
      fall_resistance : 0.0523;
      related_pin : "CP";
    }
  }
                  /**********************************/
                  /* Output Power for Q Output      */
                  /**********************************/
		  cell_leakage_power : 1; 
		  internal_power(output_by_cap) { 
		     values("36.000000 , 5.000000 , 2.000000 ");
		     related_outputs : "Q"; 
		  }
  pin(QN) {
    direction : output;
    function : "IQN"
    timing() {
      timing_type : rising_edge;
      intrinsic_rise : 1.59;
      intrinsic_fall : 1.57;
      rise_resistance : 0.1458;
      fall_resistance : 0.0523;
      related_pin : "CP";
    }
  }
                  /**********************************/
                  /* Output Power for QN Output      */
                  /**********************************/
		  internal_power(output_by_cap) { 
		     values("36.000000 , 5.000000 , 2.000000 ");
		     related_outputs : "QN"; 
		  }
}
cell(latch1) {
  area : 5;
  pin(D) {
    direction : input;
    capacitance : 1;
    timing() {
      timing_type : setup_falling;
      intrinsic_rise : 0.4;
      intrinsic_fall : 0.4;
      related_pin : "G";
    }
    timing() {
      timing_type : hold_falling;
      intrinsic_rise : 0.4;
      intrinsic_fall : 0.4;
      related_pin : "G";
    }
  }
  pin(G) {
    direction : input;
    capacitance : 1;
    min_pulse_width_high : 1.5;
    min_pulse_width_low : 1.5;
  }
                  /**********************************/
                  /* Internal Power for Enable Input */
                  /**********************************/
		  internal_power(input_by_trans) { 
		     values("0.550000 , 0.600000 , .700000 ");
		     related_input : "G"; 
		  }
  latch ("IQ","IQN") {
    enable : "G";
    data_in : "D";
  }
  pin(Q) {
    direction : output;
    function : "IQ";
    timing() {
      timing_type : rising_edge;
      intrinsic_rise : 0.89;
      intrinsic_fall : 0.86;
      rise_resistance : 0.1458;
      fall_resistance : 0.0653;
      related_pin : "G";
    }
    timing() {
      timing_sense : positive_unate;
      intrinsic_rise : 0.89;
      intrinsic_fall : 0.86;
      rise_resistance : 0.1458;
      fall_resistance : 0.0653;
      related_pin : "D";
    }
  }
                  /**********************************/
                  /* Output Power for Q Output      */
                  /**********************************/
		  cell_leakage_power : 1; 
		  internal_power(output_by_cap) { 
		     values("36.000000 , 5.000000 , 2.000000 ");
		     related_outputs : "Q"; 
		  }
  pin(QN) {
    direction : output;
    function : "IQN";
    timing() {
      timing_type : rising_edge;
      intrinsic_rise : 1.18;
      intrinsic_fall : 1.37;
      rise_resistance : 0.1443;
      fall_resistance : 0.0523;
      related_pin : "G";
    }
    timing() {
      timing_sense : negative_unate;
      intrinsic_rise : 1.18;
      intrinsic_fall : 1.37;
      rise_resistance : 0.1443;
      fall_resistance : 0.0523;
      related_pin : "D";
    }
  }
                  /**********************************/
                  /* Output Power for Q Output      */
                  /**********************************/
		  internal_power(output_by_cap) { 
		     values("36.000000 , 5.000000 , 2.000000 ");
		     related_outputs : "QN"; 
		  }
}
